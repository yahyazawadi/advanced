<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.FilterWindowController"
            prefHeight="200.0" prefWidth="400.0" >
    <children>
        <ChoiceBox fx:id="priceMenu" layoutX="50.0" layoutY="50.0" prefWidth="300.0" />
        <ChoiceBox fx:id="typeMenu" layoutX="50.0" layoutY="100.0" prefWidth="300.0" />
        <Button fx:id="applyFilterButton" layoutX="150.0" layoutY="150.0" text="Apply" onAction="#applyFilter" />
    </children>
</AnchorPane>
